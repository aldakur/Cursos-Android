<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="service.webservice.facilito.codigo.com.ejemplocfwebservices">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GetAndPostActivity" />
        <activity android:name=".XmlActivity" />
        <activity android:name=".JsonActivity" />
        <activity android:name=".JsonSecureActivity" />
        <activity android:name=".DataInListViewActivity" />
        <activity android:name=".DataInRecyclerViewActivity" />
        <activity android:name=".ThreadChangesInterfaceActivity" />
        <activity android:name=".ChangeTextViewSizeWithAsyncActivity" />
        <activity android:name=".TaskInParallelModeActivity"></activity>
    </application>

</manifest>